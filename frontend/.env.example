# Backend API Configuration (Update with your server IP/domain)
VITE_BACKEND_URL=http://localhost:5000

# For Production: Replace localhost with your server IP
# VITE_BACKEND_URL=http://192.168.1.100:5000
# VITE_BACKEND_URL=http://your-server-domain.com:5000

# Environment
VITE_NODE_ENV=development

# Payment Gateway (Frontend Keys)
VITE_RAZORPAY_KEY_ID=your_razorpay_key_id_here

# Google Maps API for venue selection
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Optional: Analytics/Tracking IDs
# VITE_GOOGLE_ANALYTICS_ID=your_ga_id
# VITE_HOTJAR_ID=your_hotjar_id

# Optional: Social Auth
# VITE_GOOGLE_CLIENT_ID=your_google_client_id
# VITE_FACEBOOK_APP_ID=your_facebook_app_id